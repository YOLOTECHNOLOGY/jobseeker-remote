{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": [
      "gapi",
      "gapi.auth2"
    ],
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "store/index.js",
    "store/**/*.js",
    "store/**/**/*.js",
    "pages/*.tsx",
    "pages/**/*.tsx",
    "pages/*.jsx",
    "pages/**/*.jsx",
    "components/Chat/interview/detail/index.tsx",
    ".next/types/**/*.ts",
    "/Users/mao/codes/bossjob-jobseeker-client/.next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
