{
  "name": "next-fe-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:development": "env-cmd -f .env.devsite --use-shell MAINTENANCE=$MAINTENANCE next build",
    "build:production": "env-cmd -f .env.production --use-shell MAINTENANCE=$MAINTENANCE next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit --skipLibCheck",
    "postinstall": "husky install",
    "prepare": "husky install",
    "commitmsg": "node node_modules/commitplease .git/COMMIT_EDITMSG"
  },
  "commitplease": {
    "limits": {
      "firstLine": "72",
      "otherLine": "80"
    },
    "style": "jquery",
    "component": true,
    "components": [
      "Build",
      "Fix",
      "Develop",
      "Refactor",
      "Delete",
      "Merge",
      "Test",
      "Update"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.63",
    "@mui/material": "^5.1.1",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "connected-next-router": "^4.1.0",
    "cookie-universal": "^2.1.5",
    "date-fns": "^2.23.0",
    "embla-carousel-react": "^6.2.0",
    "embla-carousel-autoplay": "^6.2.0",
    "env-cmd": "^10.1.0",
    "escape-html": "^1.0.3",
    "gapi-script": "^1.2.0",
    "is-hotkey": "^0.2.0",
    "moment": "^2.29.1",
    "next": "11.1.0",
    "next-redux-wrapper": "^7.0.5",
    "next-useragent": "^2.8.0",
    "query-string": "^7.1.0",
    "react": "17.0.2",
    "react-content-loader": "^6.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.19.5",
    "react-linkedin-login-oauth2": "^2.0.0",
    "react-redux": "^7.2.6",
    "react-router-redux": "^4.0.8",
    "react-saga": "^0.3.1",
    "react-share": "^4.4.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "sass": "^1.43.4",
    "sitemap": "^7.0.0",
    "slate": "^0.72.3",
    "slate-history": "^0.66.0",
    "slate-react": "^0.72.6",
    "slugify": "^1.6.2",
    "ua-parser-js": "^1.0.2",
    "react-device-detect": "^2.2.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "commitplease": "^3.2.0",
    "eslint": "7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
