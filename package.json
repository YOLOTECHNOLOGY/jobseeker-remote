{
  "name": "next-fe-boilerplate",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_HOST_PATH='http://localhost:3004' NODE_OPTIONS='--max_old_space_size=12000 --inspect' next dev  -p 3004   ",
    "build:development": "NODE_OPTIONS=--max_old_space_size=12000 env-cmd -f .env.devsite --use-shell MAINTENANCE=$MAINTENANCE next build",
    "build:staging": "NODE_OPTIONS=--max_old_space_size=12000 env-cmd -f .env.production --use-shell MAINTENANCE=$MAINTENANCE next build",
    "build:production": "NODE_OPTIONS=--max_old_space_size=12000 env-cmd -f .env.production --use-shell MAINTENANCE=$MAINTENANCE next build",
    "analyze": "ANALYZE=true yarn build:production",
    "start": "NODE_OPTIONS='--max_old_space_size=12000 --inspect=9230' next start",
    "lint": "next lint ",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit --skipLibCheck",
    "postinstall": "husky install",
    "prepare": "husky install",
    "commitmsg": "node node_modules/commitplease .git/COMMIT_EDITMSG",
    "test": "NODE_OPTIONS=--experimental-vm-modules env=test yarn jest",
    "crowdin:pull": "crowdin pull --all",
    "crowdin:pushT": "crowdin push translations",
    "crowdin:pushS": "crowdin push sources",
    "crowdin:status": "crowdin status translation"
  },
  "commitplease": {
    "limits": {
      "firstLine": "72",
      "otherLine": "80"
    },
    "style": "jquery",
    "component": true,
    "components": [
      "Build",
      "Fix",
      "Develop",
      "Refactor",
      "Delete",
      "Merge",
      "Test",
      "Update"
    ]
  },
  "dependencies": {
    "@crowdin/cli": "^3.11.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@formatjs/intl-localematcher": "0.2.32",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.63",
    "@mui/material": "^5.1.1",
    "@next/bundle-analyzer": "^13.2.1",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "axios": "^0.24.0",
    "braintree-web": "^3.88.1",
